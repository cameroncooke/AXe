{
  "metadata": {
    "name": "Batch Multi-Screen Example",
    "description": "Test multiple screens with batched operations - demonstrates batch mode efficiency",
    "version": "1.0",
    "author": "AXe Examples"
  },
  "settings": {
    "maxExecutionTime": 60.0,
    "stopOnError": false
  },
  "events": [
    {
      "id": "test-tap-screen",
      "type": "tap",
      "coordinates": {
        "x": 200,
        "y": 300
      },
      "delayAfter": 1.0,
      "description": "Navigate to Tap Test"
    },
    {
      "id": "tap-test-1",
      "type": "tap",
      "coordinates": {
        "x": 100,
        "y": 200
      },
      "delayAfter": 0.3,
      "description": "Tap test 1"
    },
    {
      "id": "tap-test-2",
      "type": "tap",
      "coordinates": {
        "x": 300,
        "y": 200
      },
      "delayAfter": 0.3,
      "description": "Tap test 2"
    },
    {
      "id": "tap-test-3",
      "type": "tap",
      "coordinates": {
        "x": 200,
        "y": 400
      },
      "delayAfter": 0.5,
      "description": "Tap test 3"
    },
    {
      "id": "back-to-menu-1",
      "type": "tap",
      "coordinates": {
        "x": 50,
        "y": 100
      },
      "delayAfter": 1.0,
      "description": "Back to main menu"
    },
    {
      "id": "test-text-input",
      "type": "tap",
      "coordinates": {
        "x": 200,
        "y": 450
      },
      "delayAfter": 1.0,
      "description": "Navigate to Text Input"
    },
    {
      "id": "focus-text-field",
      "type": "tap",
      "coordinates": {
        "x": 200,
        "y": 300
      },
      "delayAfter": 0.5,
      "description": "Focus text field"
    },
    {
      "id": "type-batch-test",
      "type": "type",
      "text": "Batch mode test",
      "delayAfter": 0.5,
      "description": "Type test message"
    },
    {
      "id": "clear-text",
      "type": "key",
      "keyCode": 42,
      "modifiers": ["cmd"],
      "delayAfter": 0.3,
      "description": "Select all text"
    },
    {
      "id": "delete-text",
      "type": "key",
      "keyCode": 51,
      "delayAfter": 0.5,
      "description": "Delete selected text"
    },
    {
      "id": "back-to-menu-2",
      "type": "tap",
      "coordinates": {
        "x": 50,
        "y": 100
      },
      "delayAfter": 1.0,
      "description": "Back to main menu"
    },
    {
      "id": "test-touch-control",
      "type": "tap",
      "coordinates": {
        "x": 200,
        "y": 330
      },
      "delayAfter": 1.0,
      "description": "Navigate to Touch Control"
    },
    {
      "id": "touch-down-1",
      "type": "touchDown",
      "coordinates": {
        "x": 150,
        "y": 300
      },
      "delayAfter": 0.5,
      "description": "Touch down event 1"
    },
    {
      "id": "touch-up-1",
      "type": "touchUp",
      "coordinates": {
        "x": 150,
        "y": 300
      },
      "delayAfter": 0.3,
      "description": "Touch up event 1"
    },
    {
      "id": "touch-down-2",
      "type": "touchDown",
      "coordinates": {
        "x": 250,
        "y": 400
      },
      "delayAfter": 0.8,
      "description": "Touch down event 2"
    },
    {
      "id": "touch-up-2",
      "type": "touchUp",
      "coordinates": {
        "x": 250,
        "y": 400
      },
      "delayAfter": 0.5,
      "description": "Touch up event 2"
    },
    {
      "id": "back-to-menu-3",
      "type": "tap",
      "coordinates": {
        "x": 50,
        "y": 100
      },
      "delayAfter": 1.0,
      "description": "Back to main menu"
    },
    {
      "id": "test-key-press",
      "type": "tap",
      "coordinates": {
        "x": 200,
        "y": 500
      },
      "delayAfter": 1.0,
      "description": "Navigate to Key Press"
    },
    {
      "id": "key-test-1",
      "type": "key",
      "keyCode": 4,
      "delayAfter": 0.5,
      "description": "Press 'A' key"
    },
    {
      "id": "key-test-2",
      "type": "key",
      "keyCode": 22,
      "delayAfter": 0.5,
      "description": "Press 'S' key"
    },
    {
      "id": "key-test-3",
      "type": "key",
      "keyCode": 7,
      "delayAfter": 0.5,
      "description": "Press 'D' key"
    },
    {
      "id": "key-test-4",
      "type": "key",
      "keyCode": 9,
      "delayAfter": 1.0,
      "description": "Press 'F' key"
    },
    {
      "id": "final-back-to-menu",
      "type": "tap",
      "coordinates": {
        "x": 50,
        "y": 100
      },
      "delayAfter": 0.5,
      "description": "Return to main menu"
    }
  ]
}

